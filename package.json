{
  "name": "restful-web-service-nem",
  "version": "1.0.0",
  "description": "restfull web service express and mongo",
  "main": "app.js",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.7.13",
    "nodemon": "^2.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1"
  },
  "scripts": {
    "start": "nodemon app.js",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rehiven/restfull-webservice-nem.git"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  },
  "author": "rehiven",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rehiven/restfull-webservice-nem/issues"
  },
  "homepage": "https://github.com/rehiven/restfull-webservice-nem#readme"
}
