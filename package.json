{
  "name": "restful-web-service-nem",
  "version": "1.0.0",
  "description": "restfull web service express and mongo",
  "main": "app.js",
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "mongoose": "^5.7.13",
    "nodemon": "^2.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "mocha": "^6.2.2",
    "should": "^13.2.3",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "start": "nodemon app.js",
    "lint": "eslint .",
    "test": "mocha tests/**/*Tests.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rehiven/restfull-webservice-nem.git"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  },
